{
    "name": "cmixin/business-day",
    "description": "Carbon mixin to handle business days",
    "type": "library",
    "require": {
        "php": ">=7.1",
        "nesbot/carbon": "^2.0",
        "ext-calendar": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "Cmixin\\": "src/Cmixin/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "KyleKatarn",
            "email": "carbon@selfbuild.fr"
        }
    ],
    "minimum-stability": "dev",
    "scripts": {
        "types": "php types.php"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "laravel": {
            "providers": [
                "Cmixin\\BusinessDay\\Laravel\\ServiceProvider"
            ]
        }
    }
}
